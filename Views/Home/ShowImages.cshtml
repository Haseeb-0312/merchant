@model List<The_Merchant.Models.Image>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <h1>Images</h1>
            </tr>
        </thead>
        <tbody>
            <tr>
                @foreach (var v in Model)
                {

                    <td><img src="~/Content/images/@v.img" height="150px" width="120px" class="thumbnail" /></td>


                }
            </tr>
        </tbody>
    </table>
